{% extends "TallerReparacioBackendBundle:Default:index.html.twig " %}
{% block title %}BACKEND - Categoria Afegida{% endblock %}
{% block page_content %}
    <!-- Page Content -->
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-lg-12 col-md-12">
                <h4 class="">{{ titol }}</h4>
            </div>
            <div class="col-sm-3 col-lg-3 col-md-3 text-center">
                <img class="img-responsive" src={{ client.foto }} style="width:100%;" />
                <h3>{{ client.nom }} {{ client.cognom }}</h3>
                <p>{{ client.nif }}</p>
            </div>
        </div>
    </div>
{% endblock %}
