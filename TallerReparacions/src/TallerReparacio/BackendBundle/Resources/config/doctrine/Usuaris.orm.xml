      <unique-constraint name="UNIQ_3664EC126C6E55B5" columns="nom"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="20" nullable="false">
